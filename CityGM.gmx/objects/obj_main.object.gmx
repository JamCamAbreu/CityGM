if (updateSurfaces) {
    scr_setSurfaceMiniMap(tileString, tileStringSize, id);
    
    if (curSpecialMap == MENU_MAP_POLLUTION)
        scr_setSurfacePollution(id);
    else if (curSpecialMap == MENU_MAP_LANDVALUE)
        scr_setSurfaceLandValue(id);
    
    scr_updateAll(id)
    updateSurfaces = false;
}
